


<h1>  商品新規登録画面</h1>
  <%= form_with model: @item, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image_id %>
  <h4>商品名</h4>
  <%= f.text_field :name %>
  <h4>商品説明文</h4>
  <%= f.text_area :introduction %>
  <h4>ジャンル</h4>
  <%= f.text_field :name %>
  <h4>価格</h4>
  <%= f.text_field :price %>
  <%= f.submit '投稿' %>
 <% end %>


